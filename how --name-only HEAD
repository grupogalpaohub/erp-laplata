[32m+[m[32mimport { useState, useEffect } from 'react'[m
[32m+[m[32mimport { supabaseServer } from '@/lib/supabaseServer'[m
[32m+[m[32mimport { getTenantId } from '@/lib/auth'[m
[32m+[m[32mimport { KpiData } from '@/types'[m
[32m+[m
[32m+[m[32mexport function useKpiData() {[m
[32m+[m[32m  const [data, setData] = useState<KpiData>({[m
[32m+[m[32m    totalMaterials: 0,[m
[32m+[m[32m    totalVendors: 0,[m
[32m+[m[32m    totalOrders: 0,[m
[32m+[m[32m    totalValue: 0,[m
[32m+[m[32m    totalSales: 0,[m
[32m+[m[32m    totalInventory: 0,[m
[32m+[m[32m    totalProfit: 0[m
[32m+[m[32m  })[m
[32m+[m[32m  const [loading, setLoading] = useState(true)[m
[32m+[m[32m  const [error, setError] = useState<string | null>(null)[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    async function fetchKpiData() {[m
[32m+[m[32m      try {[m
[32m+[m[32m        setLoading(true)[m
[32m+[m[32m        setError(null)[m
[32m+[m[41m        [m
[32m+[m[32m        const supabase = supabaseServer()[m
[32m+[m[32m        const tenantId = await getTenantId()[m
[32m+[m
[32m+[m[32m        // Buscar dados em paralelo[m
[32m+[m[32m        const [materialsResult, vendorsResult, ordersResult, salesResult, inventoryResult] = await Promise.allSettled([[m
[32m+[m[32m          supabase[m
[32m+[m[32m            .from('mm_material')[m
[32m+[m[32m            .select('mm_material, purchase_price_cents, sale_price_cents')[m
[32m+[m[32m            .eq('tenant_id', tenantId),[m
[32m+[m[32m          supabase[m
[32m+[m[32m            .from('mm_vendor')[m
[32m+[m[32m            .select('vendor_id, is_active')[m
[32m+[m[32m            .eq('tenant_id', tenantId),[m
[32m+[m[32m          supabase[m
[32m+[m[32m            .from('mm_purchase_order')[m
[32m+[m[32m            .select('total_amount_cents, status')[m
[32m+[m[32m            .eq('tenant_id', tenantId),[m
[32m+[m[32m          supabase[m
[32m+[m[32m            .from('sd_sales_order')[m
[32m+[m[32m            .select('total_final_cents, order_date')[m
[32m+[m[32m            .eq('tenant_id', tenantId)[m
[32m+[m[32m            .gte('order_date', new Date(new Date().getFullYear(), new Date().getMonth(), 1).toISOString().split('T')[0]),[m
[32m+[m[32m          supabase[m
[32m+[m[32m            .from('wh_inventory_balance')[m
[32m+[m[32m            .select('qty_on_hand, unit_cost_cents')[m
[32m+[m[32m            .eq('tenant_id', tenantId)[m
[32m+[m[32m            .gt('qty_on_hand', 0)[m
[32m+[m[32m        ])[m
[32m+[m
[32m+[m[32m        // Processar resultados[m
[32m+[m[32m        const materials = materialsResult.status === 'fulfilled' ? (materialsResult.value.data || []) : [][m
[32m+[m[32m        const vendors = vendorsResult.status === 'fulfilled' ? (vendorsResult.value.data || []) : [][m
[32m+[m[32m        const orders = ordersResult.status === 'fulfilled' ? (ordersResult.value.data || []) : [][m
[32m+[m[32m        const sales = salesResult.status === 'fulfilled' ? (salesResult.value.data || []) : [][m
[32m+[m[32m        const inventory = inventoryResult.status === 'fulfilled' ? (inventoryResult.value.data || []) : [][m
[32m+[m
[32m+[m[32m        // Calcular KPIs[m
[32m+[m[32m        const totalMaterials = materials.length[m
[32m+[m[32m        const totalVendors = vendors.filter(v => v.is_active).length[m
[32m+[m[32m        const totalOrders = orders.length[m
[32m+[m[32m        const totalValue = orders.reduce((sum, order) => sum + (order.total_amount_cents || 0), 0)[m
[32m+[m[32m        const totalSales = sales.reduce((sum, sale) => sum + (sale.total_final_cents || 0), 0)[m
[32m+[m[32m        const totalInventory = inventory.reduce((sum, item) => sum + (item.qty_on_hand * (item.unit_cost_cents || 0)), 0)[m
[32m+[m[32m        const totalProfit = totalSales - totalValue[m
[32m+[m
[32m+[m[32m        setData({[m
[32m+[m[32m          totalMaterials,[m
[32m+[m[32m          totalVendors,[m
[32m+[m[32m          totalOrders,[m
[32m+[m[32m          totalValue,[m
[32m+[m[32m          totalSales,[m
[32m+[m[32m          totalInventory,[m
[32m+[m[32m          totalProfit[m
[32m+[m[32m        })[m
[32m+[m[32m      } catch (err) {[m
[32m+[m[32m        console.error('Error fetching KPI data:', err)[m
[32m+[m[32m        setError(err instanceof Error ? err.message : 'Erro desconhecido')[m
[32m+[m[32m      } finally {[m
[32m+[m[32m        setLoading(false)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    fetchKpiData()[m
[32m+[m[32m  }, [])[m
[32m+[m
[32m+[m[32m  return { data, loading, error, refetch: () => fetchKpiData() }[m
[32m+[m[32m}[m
